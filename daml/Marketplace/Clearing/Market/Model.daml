module Marketplace.Clearing.Market.Model where

template StandingObservationAddRequest
  with
    provider : Party
    customer : Party
  where
    signatory provider

    controller provider can
      AddFinished : ()
        do return ()

template StandingObservationRemoveRequest
  with
    provider : Party
    customer : Party
  where
    signatory provider

    controller provider can
      RemoveFinished : ()
        do return ()
